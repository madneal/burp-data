<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.vo.Va" table="va" schema="vulgame">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="vaname">
            <column name="vaname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="vadesc">
            <column name="vadesc" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="vacost">
            <column name="vacost" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>