<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.vo.Items" table="items" schema="vulgame">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="pname">
            <column name="pname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="pcost">
            <column name="pcost" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="pdenomination">
            <column name="pdenomination" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>