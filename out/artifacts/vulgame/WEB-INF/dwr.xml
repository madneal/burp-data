<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC 
 "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN" 
 "http://www.getahead.ltd.uk/dwr/dwr10.dtd">

<!-- creator="new" new意味着DWR应当调用类的默认构造函数来获得实例 -->
<!-- javascript="testDwr" 属性指定javascript代码访问对象时使用的名称 --> 
<!-- param 指定要暴露给JAVASCRIPT访问的类 --> 
<!-- include 指定当前类中公开的方法 -->                   
<dwr>

  <allow>
      <create creator="spring" javascript="UsersService">
       <param name="beanName" value="usersService"></param>        
      </create> 
      <create creator="spring" javascript="adminService">
       <param name="beanName" value="adminService"></param>        
      </create> 
  </allow>
  
</dwr>